{
  "name": "contributte/qa",
  "description": "Tuned & very strict coding standards for PHP projects. Trusted by Contributte, Apitte, Nettrine and many others.",
  "keywords": ["codesniffer", "codestyle", "contributte", "qa", "quality assurance"],
  "type": "library",
  "license": ["MIT"],
  "homepage": "https://github.com/contributte/qa",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.4",
    "slevomat/coding-standard": "^6.4.1",
    "squizlabs/php_codesniffer": "^3.3.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "bin": [
    "bin/codesniffer",
    "bin/codefixer"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
